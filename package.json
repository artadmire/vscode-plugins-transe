{
  "name": "demo",
  "displayName": "demo",
  "description": "demo",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.toLowerCase",
        "title": "转化为大写"
      },
      {
        "command": "extension.toUpperCase",
        "title": "转化为小写"
      },
      {
        "command": "extension.toCameCase",
        "title": "转化为驼峰"
      },
      {
        "command": "extension.toSnakeCase",
        "title": "转化为下划线"
      }
    ],
    "keybindings": [
      {
        "command": "extension.toLowerCase",
        "key": "ctrl+h",
        "mac": "cmd+h",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.toUpperCase",
        "key": "ctrl+j",
        "mac": "cmd+j",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.toCameCase",
        "key": "ctrl+k",
        "mac": "cmd+k",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.toSnakeCase",
        "key": "ctrl+l",
        "mac": "cmd+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "rollup": "^3.28.1",
    "typescript": "^5.1.3"
  }
}
